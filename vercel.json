{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "headers": {
        "X-Robots-Tag": "all",
        "X-Content-Type-Options": "nosniff"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/programas",
      "destination": "/"
    },
    {
      "source": "/sistemas",
      "destination": "/"
    },
    {
      "source": "/juegos",
      "destination": "/"
    },
    {
      "source": "/extras",
      "destination": "/"
    },
    {
      "source": "/apks",
      "destination": "/"
    },
    {
      "source": "/apps",
      "destination": "/"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}