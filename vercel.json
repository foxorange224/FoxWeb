{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "all"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/(index.js|index.css|data.js)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/programas",
      "destination": "/index.html"
    },
    {
      "source": "/apps",
      "destination": "/index.html"
    },
    {
      "source": "/sistemas",
      "destination": "/index.html"
    },
    {
      "source": "/juegos",
      "destination": "/index.html"
    }
  ]
}