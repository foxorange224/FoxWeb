{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.txt",
      "use": "@vercel/static"
    },
    {
      "src": "*.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/chat\\.html",
      "dest": "/chat.html",
      "headers": {
        "Cache-Control": "public, max-age=300, s-maxage=3600",
        "Content-Type": "text/html; charset=utf-8",
        "X-Robots-Tag": "noindex, nofollow"
      }
    },
    {
      "src": "/data\\.js",
      "dest": "/data.js",
      "headers": {
        "Cache-Control": "public, max-age=3600, s-maxage=86400",
        "Content-Type": "application/javascript; charset=utf-8",
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/ads\\.txt",
      "dest": "/ads.txt",
      "headers": {
        "Cache-Control": "public, max-age=86400, s-maxage=604800",
        "Content-Type": "text/plain; charset=utf-8",
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/robots\\.txt",
      "dest": "/robots.txt",
      "headers": {
        "Cache-Control": "public, max-age=86400, s-maxage=604800",
        "Content-Type": "text/plain; charset=utf-8",
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*\\.(js|css))",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "Access-Control-Allow-Origin": "*"
      },
      "continue": true
    },
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|ico|svg|webp|woff|woff2|ttf|eot))",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "Access-Control-Allow-Origin": "*"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "public, max-age=0, s-maxage=3600",
        "Content-Type": "text/html; charset=utf-8"
      }
    }
  ],
  "redirects": [
    {
      "source": "/chat",
      "destination": "/chat.html",
      "permanent": true
    },
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/inicio",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/programas",
      "destination": "/#Programas",
      "permanent": false
    },
    {
      "source": "/sistemas",
      "destination": "/#Sistemas",
      "permanent": false
    },
    {
      "source": "/juegos",
      "destination": "/#Juegos",
      "permanent": false
    },
    {
      "source": "/extras",
      "destination": "/#Extras",
      "permanent": false
    },
    {
      "source": "/apks",
      "destination": "/#APKs",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
}