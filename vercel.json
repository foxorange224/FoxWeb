{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "all"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "src": "/(index.js|index.css|data.js|sitemap.xml|robots.txt)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/programas",
      "destination": "/"
    },
    {
      "source": "/sistemas",
      "destination": "/"
    },
    {
      "source": "/juegos",
      "destination": "/"
    },
    {
      "source": "/extras",
      "destination": "/"
    },
    {
      "source": "/apks",
      "destination": "/"
    },
    {
      "source": "/apps",
      "destination": "/"
    }
  ]
}